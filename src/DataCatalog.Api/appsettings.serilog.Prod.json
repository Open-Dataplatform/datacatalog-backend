{
  "Serilog": {
    "Using": [ "Serilog.Sinks.ElasticSearch" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "BookingPlatform.Infrastructure.ServiceBase.SerilogCustomJsonFormatter, BookingPlatform.Infrastructure.ServiceBase"
        }
      },
      {
        "Name": "ElasticSearch",
        "Args": {
          "nodeUris": "http://dptelemetry-prod.westeurope.cloudapp.azure.com:9200/"
        }
      }
    ]
  }
}
