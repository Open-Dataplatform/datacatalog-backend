{
  "OAuth": {
    "Audience": "ignored"
  },
  "AllowedCorsUrls": {
    "EgressApiUrl": "ignored",
    "IngressApiUrl": "ignored",
    "DataCatalogUrl": "ignored",
    "DataCatalogProdUrl": "ignored"
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {NewLine}{Exception}"
        }
      }
    ]
  },
  "AzureAd": {
    "Enabled": false
  }
}